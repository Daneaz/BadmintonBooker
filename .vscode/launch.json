    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Single Booker",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/BookerForParticularCC.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "List Booker",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/BadmintonBooking.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Open Browser With Cookies",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/OpenBroswerWithCookies.js"
        },
        {
            "type": "node",
            "request": "launch",
            "outputCapture": "std",
            "name": "Backend",
            "program": "${workspaceFolder}/bin/www",
            "env": {
                "PORT": "9999",
            }
        }
    ]
}